{{>site-header}}
  <div class="page-content">
  <h2>{{ this.page.title }}</h2>
  {{#markdown}}
    {{> body}}
  {{/markdown}}
    <h1>Post (Blog) Contents</h1>
    <ul>
    {{#withSort post 'data.posted'}}
      {{#each this.pages }}
      <li><a href="{{ replaceStr this.dest 'dist' '' }}">{{ this.data.title }}</a></li>
      {{/each}}
    {{/withSort}}
    </ul>

    <h1>Another Post Type (Gallery) contents</h1>
    <ul>
    {{#withSort gallery 'data.posted'}}
      {{#each this.pages }}
      <li><a href="{{ replaceStr this.dest 'dist' '' }}">{{ this.data.title }}</a></li>
      {{/each}}
    {{/withSort}}
    </ul>
  </div>
{{>site-footer}}